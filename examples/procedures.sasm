;; simple example with procedures
;; helper definitions for some registers
#define A 0
#define B 1
#define C 2

;; main programm starting here
LOADI A, 777
LOADI B, 888
LOADI C, 999
PRINT A
CALL 9
PRINT B
CALL 9
PRINT C
HALT

;; beginning of procedure1
LOADI A, 0
LOADI B, 1
LOADI C, 2
PRINT A
PRINT B
PRINT C
RET